    "23cc1dc9-e7a7-4c71-a3a9-a7f1044eda63": {
      "type": "dynamic-html",
      "settings": {
        "title": "",
        "custom_html": "<style>\n\/* calculator styles *\/\n.calc-main-column{\nfloat:left;\nwidth:69%;\n\n}\n.calc-main-table{\n\tfloat:right;\n\twidth:30%;\n}\n\n\n.calc-main-column label{\n\tdisplay:block;\n\tpadding:5px;\n}\n\n.calc-main-column .input-text{\n\tfont-size:18px;\n\tline-height:20px;\n\tpadding:5px;\n}\n\n\n.calc-main-table .table{\nborder-spacing:0;\nwidth:100%\n}\n.calc-main-table .table thead > tr > th{\nbackground-color:#303e49;\ncolor:#fff;\nfont-size:15px;\npadding:5px;\n}\n.calc-main-table .table thead > tr > th\n.calc-main-table .table tbody > tr > th, .table tfoot > tr > th,\n.calc-main-table .table thead > tr > td, .table tbody > tr > td,\n.calc-main-table .table tfoot > tr > td{\n\tborder-top:1px solid #dddddd;\n}\n.table-striped > tbody > tr:nth-child(2n+1) > td,\n.table-striped > tbody > tr:nth-child(2n+1) > th{\n\tbackground-color:#f9f9f9;\n}\n\n@media only screen and (max-width:550px){\n\t.calc-main-table{\n\t\tfloat:none;\n\t\twidth:100%;\n\t}\n\t.calc-main-column{\n\tfloat:none;\n\twidth:100%;\n\n\t}\n\n}\n\n\n\n<\/style>"
