{% style %}
  .footer-icon-grid {
    display: flex;
    justify-content: space-between;
    padding: 26px 37px;
    flex-wrap: wrap;
    border-radius: 65px;
    background: linear-gradient(298.47deg, #F3F6F9 0%, #E5E8EF 100%);
    margin-bottom: 30px;
    gap: 40px 20px;
    position: relative;
  }

  .footer-icon {
    flex-basis: 45%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
  }

  .footer-icon svg {
    height: 40px;
    width: 50px;
    color: {{ settings.colors.icons }};
  }

  .footer-text {
    color: {{ settings.color_dark_gray }};
    text-align: center;
    font-size: 16px;
    line-height: 20px;
  }
  @media (max-width: 1024px){
    .footer-icon-grid:before {
      content: '';
      position: absolute;
      background: {{ settings.colors.section_background }};
      width: 1px;
      height: calc(100% - 20px);
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }

    .footer-icon-grid:after {
      content: '';
      position: absolute;
      background: {{ settings.colors.section_background }};
      width: calc(100% - 20px);
      height: 1px;
      top: 50%;
      left: 50%;
      transform: translate(-50%);
    }
    .site-footer-block-social-accounts{
      text-align: center;
    }
  }
  @media (min-width: 1024px){
    .footer-icon-grid {
      display: flex;
      justify-content: space-between;
      padding: 26px 54px;
      border-radius: 65px;
      background: linear-gradient(298.47deg, #F3F6F9 0%, #E5E8EF 100%);
      margin-bottom: 65px;
    }

    .footer-icon {
      flex-basis: 22%;
      display: flex;
      gap: 20px;
      flex-direction: row;
      align-items: center;
    }

    .footer-icon svg {
      height: 40px;
      width: 50px;
      color:  {{ settings.colors.icons }};
    }

    .footer-text {
      color:  {{ settings.colors.text }};
      font-size: 16px;
      line-height: 20px;
      text-align: left;
    }
  }

{% endstyle %}

