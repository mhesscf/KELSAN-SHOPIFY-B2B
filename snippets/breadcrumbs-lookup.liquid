{%- unless template == 'index' or template == 'cart'  or template == '404' or template == null -%}
    {%- assign current_website = 'cf' -%}
    {%- if request.host contains 'airmovers'  -%}
        {%- assign current_website = 'am' -%}
    {%- elsif request.host contains 'carpetextractors'  -%}
        {%- assign current_website = 'ce' -%}
    {%- elsif request.host contains 'floorscrubbers'  -%}
        {%- assign current_website = 'fs' -%}
    {%- elsif request.host contains 'floorbuffers'  -%}
        {%- assign current_website = 'fb' -%}
    {%- endif -%}

    {%- comment -%}
    ---------------------------------------------------------------------------------------
    use this if we ever need to pull breadcrumbslookup from the default shopify nav
           {%- for link in linklists.breadcrumbs.links -%}
               {{ link.url }} : {{ link.url }}, <br/>
               {%- for sublink in link.links -%}
                   {{ link.url}} > {{ sublink.url }}:{{ sublink.url }},<br/>
                   {%- for subsublink in sublink.links -%}
                       {{ link.url}} > {{ sublink.url }} > {{ subsublink.url }}:{{ subsublink.url}},<br/>
                   {%- endfor -%}
               {%- endfor -%}
           {%- endfor -%}
   ---------------------------------------------------------------------------------------
   {%- endcomment -%}

    {%- assign productpage = 0 -%}
    {%- if template contains 'product' -%}
        {%- assign productpage = 1 -%}
        {%- capture product_url_string -%}
            {%- for collection in product.collections -%}
                {%- comment -%}ignore-bread tagged collections, ignore all{%- endcomment -%}
                {%- unless collection.metafields.custom.ignore_in_breadcrumbs -%}
                    {{collection.url }}||
                {%- endunless -%}
            {%- endfor -%}
        {%- endcapture -%}
        {%- comment -%}{%- assign object_url_string = product_url_string | append: product.url -%}{%- endcomment -%}
        {%- assign object_url_string = product_url_string -%}

    {%- elsif template contains 'collection' and collection.handle -%}
        {%comment%} ignore brand collection   {%endcomment%}
        {%- capture object_url_string -%}/collections/{{ collection.handle }}{%- endcapture -%}
    {%- elsif template contains 'page' -%}
        {%- capture object_url_string -%}{{ page.url }}{%- endcapture -%}
    {%- endif -%}


