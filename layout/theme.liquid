{% assign override = 0 %}
{% if customer.tags contains 'SuperFreak' or customer.tags contains 'superfreak'or override == 1 %}
    <script>
        setTimeout(() => {
            console.log("SuperFreak Eligible "+readCookieHead('superfreakPromo') +"/"+ readCookieHead('discount_code'));
            if(readCookieHead('superfreakPromo')){
                if(readCookieHead('discount_code') == "SUPERFREAK"){
                    console.log('Applying SF Discount')
                }
                if(readCookieHead('superfreakPromo') == 1){
                    $('.sf-promo-link.promo-applied').show();
                }else if(readCookieHead('superfreakPromo') == 0){
                    $('.sf-promo-link.promo-needed').show();
                }
            }else{
                $('.popup-container').removeClass('hidden');
                $(".pop-grad").removeClass('hidden');
                $(".popup-container .close-pop").addClass('sf-off');
                var newhtml = "<div style='text-align:center'><div style='margin:auto; width:230px; text-align:left'><strong><img style='float:left; margin: -8px 7px -9px 6px' src='{{ 'superfreak-logo-sm.jpg' | file_img_url  }}' alt='SuperFreak Link' />Welcome Back<br/>SuperFreak Member!</strong></div><br/>Would you like to apply your SuperFreak 10% discount on parts today?<br><br><button style='width:70px;' onClick='sfApply(1)'><span class='btn-text'>Yes</span></button> <button style='width:70px;' onClick='sfApply(0)'><span class='btn-text'>No</span></button> </div>";
                $('.popup-container .content').html(newhtml);

                $('.popup-container .close-pop.sf-off').on('click', function(e){
                    sfApply(0);
                });
            }
        }, "1000");
        function readCookieHead(name) {
            return (name = new RegExp('(?:^|;\\s*)' + ('' + name).replace(/[-[\]{}()*+?.,\\^$|#\s]/g, '\\$&') + '=([^;]*)').exec(document.cookie)) && name[1];
        }
    </script>
{% endif %}


{% assign sf-discount = 0 %}
{% assign sf-discoutItem = 0 %}
{% unless customer.tags contains 'SuperFreak' %}
    {% for cartitem in cart.items %}
        {% if cartitem.product.handle contains 'superfreak-program' %}
            {% assign sf-discount = 1 %}
        {% endif %}
        {% for c in cartitem.product.collections %}
            {% if c.handle == "superfreak-discount-eligible" %}
                {% assign sf-discoutItem = 1 %}
            {% endif %}
        {% endfor %}
    {% endfor %}
{% endunless %}

<script>
    var currentsfdis = readCookieHead2('discount_code');
    const fetchUrl = "/pages/contact?view=sf-dis";
    var sfdis = ""
    {% if sf-discount == 1 and sf-discoutItem == 1 %}
    {% comment %}get "hidden discount" {% endcomment %}
    $.get(fetchUrl).then(response => {
        if (response) {
            sfdis = response;
            if(currentsfdis != response && currentsfdis == undefined ){
                const fetchUrl = "/discount/"+response;
                $.get(fetchUrl).then(response => {
                    if (response) {
                        console.log("SF");
                    }
                });
            };
        }
    });
    {% else %}
    $.get(fetchUrl).then(response => {
        if (response) {
            if(currentsfdis == response || currentsfdis == "CLEAR" ) {
                fetch("/discount/CLEAR").then(response => {
                    fetch("/checkout?discount=%20").then(response => {
                        console.log("clear");
                        document.cookie = "discount_code=NA; expires=Thu, 01 Jan 1970 00:00:01 GMT; path=/";
                    });
                });
            };
        }
    });
    {% endif %}
    function readCookieHead2(name) {
        return (name = new RegExp('(?:^|;\\s*)' + ('' + name).replace(/[-[\]{}()*+?.,\\^$|#\s]/g, '\\$&') + '=([^;]*)').exec(document.cookie)) && name[1];
    }
</script>
{% comment %}end of superfreak{% endcomment %}

